@model IEnumerable<WorkoutPlanObjects.WorkoutSetObject>


    @foreach (var group_item in Model)
    { 
        
            if(group_item.SetType.ToString().Equals("Group"))
            {
                <tr>
                    <td>
                        @Html.DisplayFor(j => group_item.OrderID). &nbsp;
                    </td>
                    <td style="width:80%" >                        
                        @for (int i = 0; i < ViewBag.indent; i++)
                        {
                            @Html.Raw("&nbsp;&nbsp;&nbsp;&nbsp;");
                        }
                        @Html.DisplayFor(j => group_item.Repeats) X {
                    </td>
                    <td width="100px" align="right">
                        <input type="button" value="Edit Group" class="editBtn" data-div="group" data-orderid="@group_item.OrderID" data-parentid="@group_item.ParentID" onclick="editGroupBtnClicked(@group_item.OrderID,@group_item.ParentID)" />
                    </td>
                </tr>               
                ViewBag.indent++;
                @Html.Partial("_WorkoutGroup", group_item.SubSetList)
                ViewBag.indent--;
                <tr>
                    <td></td>
                    <td>                        
                    @for (int i = 0; i < ViewBag.indent; i++)
                        {
                            @Html.Raw("&nbsp;&nbsp;&nbsp;&nbsp;");
                        }
                    }</td>
                    <td></td>
                </tr>
            }
            else
            {
                 @Html.Partial("_WorkoutSet", group_item)
            }
    }

